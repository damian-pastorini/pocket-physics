{
  "name": "pocket-physics",
  "version": "7.1.2",
  "description": "Verlet physics extracted from pocket-ces demos",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.js",
  "browserify": {
    "extensions": [
      ".ts",
      ".tsx"
    ],
    "transform": [
      [
        "babelify",
        {
          "extensions": [
            ".ts"
          ]
        }
      ]
    ]
  },
  "files": [
    "esm/",
    "cjs",
    "src/",
    "types/"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "yarn build:esm && yarn build:cjs && yarn build:types",
    "build:esm": "BABEL_ENV=build-esm babel --extensions '.ts' --out-dir esm/ src",
    "build:cjs": "BABEL_ENV=build-cjs babel --extensions '.ts' --out-dir cjs/ src",
    "build:types": "tsc --build",
    "test": "babel-node --extensions .ts src/test*.ts",
    "demos": "tools/demos.sh",
    "demos:bundle": "tools/demos-bundle.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kirbysayshi/pocket-physics"
  },
  "keywords": [
    "verlet",
    "physics",
    "pocket-ces",
    "game"
  ],
  "author": "Drew Petersen <kirbysayshi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kirbysayshi/pocket-physics/issues"
  },
  "homepage": "https://github.com/kirbysayshi/pocket-physics",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@types/tape": "^4.2.33",
    "babelify": "^10.0.0",
    "beefy": "^2.1.5",
    "browserify": "^16.3.0",
    "science-halt": "^0.2.0",
    "tape": "^4.6.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {}
}

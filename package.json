{
  "name": "pocket-physics",
  "version": "7.1.2",
  "description": "Verlet physics extracted from pocket-ces demos",
  "main": "dist/index.js",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "prepublishOnly": "babel --out-dir dist/ src",
    "test": "tape -r @babel/register test*.js",
    "demos": "beefy $(for f in examples/*.js; do echo \"$f:examples/$(basename $f .js)-bundle.js \"; done)",
    "demos:bundle": "rm examples/*-bundle.js; for js in examples/*.js; do browserify $js > examples/$(basename $js .js)-bundle.js; done"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kirbysayshi/pocket-physics"
  },
  "keywords": [
    "verlet",
    "physics",
    "pocket-ces",
    "game"
  ],
  "author": "Drew Petersen <kirbysayshi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kirbysayshi/pocket-physics/issues"
  },
  "homepage": "https://github.com/kirbysayshi/pocket-physics",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "babelify": "^10.0.0",
    "beefy": "^2.1.5",
    "browserify": "^16.3.0",
    "science-halt": "^0.2.0",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "debug": "^4.1.1"
  }
}
